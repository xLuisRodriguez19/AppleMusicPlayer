{
  "name": "apple-music-player",
  "version": "0.0.0",
  "private": true,
  "main": "main.ts",
  "scripts": {
    "dev": "vite --port 8080",
    "serve": "vite preview",
    "build": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix src --ignore-path .gitignore",
    "format": "prettier --write src/",
    "electron:start": "electron . --port=8080",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dotenv": "^16.1.4",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "pinia": "^2.0.36",
    "vue": "^3.3.2",
    "vue-i18n": "^9.3.0-beta.19",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.16",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "cross-env": "^7.0.3",
    "electron": "^25.0.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.14.1",
    "husky": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "~5.0.4",
    "vite": "^4.3.5",
    "vue-tsc": "^1.6.4"
  }
}
